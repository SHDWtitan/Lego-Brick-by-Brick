{
  "config": {"view": {"continuousWidth": 300, "continuousHeight": 300}},
  "data": {
    "url": "https://raw.githubusercontent.com/SHDWtitan/Lego-Brick-by-Brick/refs/heads/Hunter_Layfield/data/lego_cleaned.csv"
  },
  "mark": {"type": "point", "tooltip": true},
  "description": "LEGO",
  "encoding": {
    "color": {"field": "theme", "type": "nominal"},
    "x": {"aggregate": "count", "field": "theme", "title": "number of sets"},
    "y": {
      "aggregate": "mean",
      "field": "pieces",
      "title": "average pieces per set"
    }
  },
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json"
}