    "import pandas as pd\n",
    "\n",
    "data_frame = pd.read_csv(r\"lego_sets.csv")\n",
    "filt_data = data_frame[(data_frame[\"year\"]>= 2000) & (data_frame[\"US_retailPrice\"]> 0) & (data_frame[\"pieces\"]>0)]\n",
    "\n",
    "print(filt_data.head())"
   ]
  },
