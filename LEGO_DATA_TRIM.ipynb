   "source": [
    "# drop unnecessary columns\n",
    "\n",
    "filt_data = filt_data.drop(columns=[\"bricksetURL\",\"thumbnailURL\",\"imageURL\",\"agerange_min\"])\n",
    "\n",
    "# fill empty values\n",
    "\n",
    "filt_data[\"minifigs\"] = pd.to_numeric(filt_data[\"minifigs\"],errors = \"coerce\").fillna(0).astype(int)"
   ]
